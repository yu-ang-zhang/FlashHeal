<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fg_diary">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EBEBEB"
        android:orientation="vertical">
        <TableLayout
            android:layout_width="430dp"
            android:layout_height="220dp"
            android:background="@drawable/diary_bg"
            android:gravity="center"
            android:layout_marginTop="-5dp"
            android:layout_gravity="center"
            android:orientation="vertical">


            <TableRow
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_marginTop="20dp"
                android:paddingTop="30dp">

                <TextView
                    android:id="@+id/eaten"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="60dp"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:textColor="@color/white"

                    android:textSize="30dp"></TextView>

                <TextView
                    android:id="@+id/kaluli"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                   android:layout_marginRight="60dp"
                    android:layout_marginTop="20dp"
                    android:textSize="30dp"></TextView>

                <TextView
                    android:id="@+id/kalulileft"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="30dp"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textSize="30dp"></TextView>
            </TableRow>

            <TableRow
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="30dp">

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="-5dp"
                    android:layout_marginTop="20dp"

                    android:gravity="center"
                    android:text="EATEN"
                    android:textColor="@color/white"
                    android:textSize="25dp"></TextView>

                <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:text="Kcal"
                    android:layout_marginTop="0dp"
                    android:layout_marginLeft="-30dp"
                    android:textColor="@color/white"
                    android:textSize="30dp"></TextView>

                <TextView
                    android:layout_width="20dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="0dp"
                    android:gravity="center"
                    android:text="LEFT"
                    android:textColor="@color/white"
                    android:textSize="25dp"></TextView>
            </TableRow>

        </TableLayout>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="572dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="0dp">

            <Button
                android:id="@+id/add_b"
                android:background="@drawable/biankang"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:drawableLeft="@drawable/bre"

                android:drawableRight="@drawable/addbutton"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="Breakfast"
                android:textColor="#717171"
                android:textSize="30dp"></Button>

            <Button
                android:id="@+id/add_l"
                android:layout_width="wrap_content"
                android:background="@drawable/biankang"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:drawableLeft="@drawable/lun"
                android:drawableRight="@drawable/addbutton"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="Lunch"
                android:textColor="#717171"
                android:textSize="30dp"></Button>

            <Button
                android:id="@+id/add_d"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/biankang"
                android:layout_marginTop="20dp"
                android:drawableLeft="@drawable/din"
                android:drawableRight="@drawable/addbutton"
                android:paddingBottom="10dp"

                android:paddingTop="10dp"
                android:text="Dinner"
                android:textColor="#717171"
                android:textSize="30dp"></Button>

            <Button
                android:id="@+id/add_s"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="@drawable/biankang"
                android:drawableLeft="@drawable/sna"
                android:drawableRight="@drawable/addbutton"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="Snack"
                android:textColor="#717171"
                android:textSize="30dp"></Button>
        </TableLayout>
    </LinearLayout>
</FrameLayout>